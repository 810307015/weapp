/* components/zhouyi/zhouyi.wxss */
.zhouyi {
  padding-top: 80rpx;
  padding-bottom: 120rpx;
}

.zhouyi .space {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100rpx;
  background-color: #FFFFFF;
}

.zhouyi .search {
  position: fixed;
  top: 30rpx;
  left: 30rpx;
  right: 30rpx;
  height: 60rpx;
  padding: 0 30rpx;
  background-color: #F8F8F8;
}

.zhouyi .list {
  padding: 30rpx;
}

.zhouyi .list .item {
  margin-bottom: 30rpx;
}